{
  "manifest_version": 3,
  "name": "Amazon Logistics Data Extractor",
  "version": "1.4.0",
  "description": "Extract journey and route data from Amazon Logistics with automatic review creation and fleet vehicle synchronization",
  "permissions": [
    "downloads",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://logistics.amazon.com/*",
    "https://vehicle-api20250712121147-cwg9d7fyeugehbc5.westus-01.azurewebsites.net/*",
    "https://localhost:*/*",
    "http://localhost:*/*"
  ],
  "action": {
    "default_popup": "popup-minimal.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
